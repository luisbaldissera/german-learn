blau - azul, "blue"
braun - castanho, marrom, "brown"
gelb - amarelo, "yellow"
grau - conza, "gray"
grün - verde, "green"
lila - roxo, "purple"
orange - laranja, "orange"
rosa - rosa, "pink"
rot - vermelho, "red"
schwarz - preto, "black"
weiß - branco, "white"

hell(farbe) - (cor) claro, "light (color)"
dunke(farbe) - (cor) escuro, "dark (color)"

